<li class="grid-x grid-padding-x text-center" data-property-index="${nextPropertyIndex}">

  <h2 class="index-title">${nextPropertyIndex}</h2>

  <a class="remove-poperty button" onclick="removeProperty(this)" >x</a>

  <div class="medium-4 cell">
      <label>Aggregation
        <select name="${nextPropertyIndex}][aggregation]">
          <option value="1" selected title="Softmax probabability vector from nerual network.">Softmax</option>
          <option value="100" title="Softmax prob vector but with lowered high values and amplified middle values.">Softmax Amplified 1</option>
          <option disabled value="101">Softmax Amplified 2</option>
          <option value="2" title="Linear scale of raw neural network data to interval from 0 to 1 based on image min/max value.">0-1 Linear Scale</option>
          <option value="200" title="Based on 0-1 Linear Scale but middle values are amplified.">Amplified 1</option>
          <option value="201" title="Based on 0-1 Linear Scale but middle values are amplified  and lower values are minified.">Amplified 2</option>
          <option disabled value="202">Amplified 3</option>
          
          
        </select>
      </label>
    </div>

  <div class="medium-4 cell">
    <label>Ranking model
      <select class="model-type-select" name="${nextPropertyIndex}][modelType]">
        <option value="0" disabled>Boolean</option>
        <option value="1" selected title="Based on Boolean model but images that do not fully satisfy formula are included with lower rank. Ranks are primarly based on number of satisfied clauses. They can be sorted within buckets as well.">Boolean Bucketed</option>
        <option value="2" disabled>Boolean Extended</option>
        <option value="3" title="VIRET tool model.">Viret Base</option>
        <option value="4" disabled>Fuzzy Logic</option>
      </select>
    </label>
  </div>

  <div class="medium-4 cell">
    <label>Data source
      <select name="${nextPropertyIndex}][dataSource]">
        <option value="0" title="Queries that has been gathered from people developing this app." selected>Developers</option>
        <option value="1" title="Queries gathered from wild public on the internet.">Public</option>
        <option disabled value="2" title="Queries gathered from public but during organized sessions.">Managed public</option>
      </select>
    </label>
  </div>

  <!-- Boolean model settings -->
  <div class="model-properties model-type-0 small-12 cell hidden-by-default">
    <div class="grid-x grid-padding-x text-center">
        <div class="small-12 cell">
            <small>Model specific settings</small>
        </div>
        <div class="medium-4 cell">
            
          <label>
            True treshold
            <input name="${nextPropertyIndex}][trueTresholds2]" type="number" value="0.005" step="0.00001">
          </label>
        </div>

    </div>
  </div>

  <!-- Boolean custom model settings -->
  <div class="model-properties model-type-1 small-12 cell">
      <div class="grid-x grid-padding-x text-center">
          <div class="small-12 cell">
            <small>Model specific settings</small>
          </div>
          <div class="medium-4 cell">
              
            <label>
              True treshold
              <input name="${nextPropertyIndex}][trueTreshold]" type="number" value="0.005" step="0.00001">
            </label>
          </div>

          <div class="medium-4 cell">
              <label>In bucket ranking
                <select class="in-bucket-ranking-select" name="${nextPropertyIndex}][inBucketRanking]">
                  <option value="0" selected>None</option>
                  <option value="1">Sum</option>
                  <option value="2">Max</option>
                </select>
              </label>
            </div>
  
      </div>
    </div>

    <!-- Boolean extended model settings -->
    <div class="model-properties model-type-2 small-12 cell hidden-by-default">
      <div class="grid-x grid-padding-x text-center">
          <div class="small-12 cell">
              <small>Model specific settings</small>
          </div>
          <div class="medium-4 cell">
              
            <label>
              True treshold
              <input name="${nextPropertyIndex}][trueTreshold3]" type="number" value="0.005" step="0.00001">
            </label>
          </div>
  
    
        </div>
      </div>

      <!-- Viret base settings -->
      <div class="model-properties model-type-3 small-12 cell hidden-by-default">
        <div class="grid-x grid-padding-x text-center">
            <div class="small-12 cell">
                <small>Model specific settings</small>
            </div>
            <div class="medium-4 cell">
                
              <label>
                True treshold
                <input name="${nextPropertyIndex}][trueTreshold4]" type="number" value="0.005" step="0.00001">
              </label>
            </div>
  
            <div class="medium-4 cell">
                <label>Query operation
                  <select class="query_operations-select" name="${nextPropertyIndex}][queryOperations]">
                    <option value="0" selected>Multiply, Add</option>
                    <option value="1">Add only</option>
                  </select>
                </label>
              </div>
    
        </div>
      </div>

</li>


